        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "& when (@showIcons)": {
              ".iconfont-wrapper": {
                "display": "flex",
                "width": "~\"@{iconsSize}rem\"",
                "height": "~\"@{iconsSize}rem\"",
                "span": {
                  "margin": "auto"
                }
              },
              ".card-social-icon": {
                "background-color": "@iconsBg",
                "color": "@iconsColor",
                "border-radius": "100%",
                "margin": "auto 3px",
                "width": "calc(0.8rem ~\"+ @{iconsSize}rem\")",
                "height": "calc(0.8rem ~\"+ @{iconsSize}rem\")",
                "font-size": "~\"@{iconsSize}rem\"",
                ".icon-wrap": {
                  "padding": "0"
                }
              },
              ".card-social-icon:hover": {
                "color": "@iconsHover",
                "transition": "all .5s",
                "svg": {
                  "fill": "@iconsHover"
                }
              }
            },
            "& when not (@showTitle)": {
