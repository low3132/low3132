              "margin": "auto",
              "text-align": "center"
            },
            ".ourteam-social": {
              "margin-top": "1.2rem"
            },
            ".card-img": {
              "margin-bottom": "25px",
              "border-radius": "100%",
              "overflow": "hidden",
              "width": "~\"@{sizeOfPicture}rem\"",
              "z-index": "100",
              "height": "~\"@{sizeOfPicture}rem\"",
              "-webkit-mask-image": "-webkit-radial-gradient(white, black)"
            },
            ".mobirise-loader": {
              "transition": "all .3s"
            },
            ".list-item:hover": {
              ".mobirise-loader": {
                "-webkit-transform": "scale(1.2, 1.2)",
                "transform": "scale(1.2, 1.2)"
              }
            },
            "@media (max-width: 991px)": {
